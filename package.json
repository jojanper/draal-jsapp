{
  "name": "draal-jsapp",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jojanper/draal-jsapp.git"
  },
  "bugs": {
    "url": "https://github.com/jojanper/draal-jsapp/issues"
  },
  "scripts": {
    "lint": "eslint **/*.js bin/www --debug",
    "start": "DEBUG=draal-jsapp:* nodemon ./bin/www -e .js --ignore node_modules",
    "start-prod": "NODE_ENV=production node ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "chalk": "^1.1.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "morgan": "~1.7.0",
    "pug": "~2.0.0-beta10",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "nodemon": "~1.11.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.1.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "object-curly-spacing": 0
    }
  }
}
